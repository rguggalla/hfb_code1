{
	"name": "RunningTotal",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "sale1_dtst",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "window1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          saledate as date 'dd-MMM-yyyy',",
				"          { saleitem} as string,",
				"          { country} as string,",
				"          { quantity} as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     dateFormats: ['dd-MMM-yyyy']) ~> source1",
				"source1 window(asc(saledate, true),",
				"     RunningTotal = sum({ quantity})) ~> window1"
			]
		}
	}
}