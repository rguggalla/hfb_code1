{
	"name": "fixedtocsv",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "DelimitedText4",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(output(",
				"          {saledate     item      country   quantity} as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> source1",
				"source1 derive(Saledate = substring({saledate     item      country   quantity}, 1, 13),",
				"          Item = substring({saledate     item      country   quantity}, 13, 23),",
				"          Country = substring({saledate     item      country   quantity}, 23, 33),",
				"          Quantity = substring({saledate     item      country   quantity}, 33, 41)) ~> derivedColumn1"
			]
		}
	}
}